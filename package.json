{
  "name": "infer",
  "version": "2.0.0",
  "description": "Infer a name from an email address",
  "main": "infer.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/*",
    "build": "browserify -o bundle.js -s infer ."
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "bin": {
    "infer": "bin/infer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bevacqua/infer.git"
  },
  "author": "Nicolas Bevacqua <nicolasbevacqua@gmail.com> (http://bevacqua.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bevacqua/infer/issues"
  },
  "homepage": "https://github.com/bevacqua/infer",
  "devDependencies": {
    "browserify": "^6.3.3",
    "tape": "^3.0.3"
  },
  "dependencies": {
    "brfs": "^1.2.0",
    "get-stdin": "^3.0.2",
    "validator": "^3.22.1"
  }
}
